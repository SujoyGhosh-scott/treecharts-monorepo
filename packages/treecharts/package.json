{
  "name": "treecharts",
  "version": "0.4.0",
  "description": "A flexible and customizable library for creating tree visualizations with multiple rendering types and advanced styling options",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.global.js",
  "jsdelivr": "dist/index.global.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "script": "./dist/index.global.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.MD",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "vite serve dev",
    "dev:watch": "tsup --watch & vite serve dev",
    "lint": "eslint src/**/*.ts",
    "test": "echo \"No tests specified\" && exit 0",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "tree",
    "chart",
    "visualization",
    "hierarchy",
    "svg",
    "diagram",
    "organizational-chart",
    "family-tree",
    "data-visualization",
    "typescript"
  ],
  "author": "Sujoy Ghosh",
  "license": "MIT",
  "homepage": "https://treecharts.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/SujoyGhosh-scott/treecharts-monorepo",
    "directory": "packages/treecharts"
  },
  "bugs": {
    "url": "https://github.com/SujoyGhosh-scott/treecharts-monorepo/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "sideEffects": false,
  "devDependencies": {
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vite": "^6.3.3"
  }
}
